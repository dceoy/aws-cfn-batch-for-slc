{
  "containerOverrides": {
    "command": [
      "--output-s3=s3://{{ output_s3_bucket_name }}/test/",
      "--outbound-sync-option='--quiet'",
      "curl",
      "-SL",
      "-o",
      "/output/global_ip.txt",
      "ifconfig.me"
    ],
    "environment": [
      {
        "name": "INPUT_DATA_DIR",
        "value": "/input"
      },
      {
        "name": "OUTPUT_DATA_DIR",
        "value": "/output"
      }
    ],
    "resourceRequirements": [
      {
        "type": "MEMORY",
        "value": "2048"
      },
      {
        "type": "VCPU",
        "value": "1"
      }
    ]
  },
  "jobDefinition": "{{ job_definition }}",
  "jobName": "curl",
  "jobQueue": "{{ job_queue }}",
  "timeout": {
    "attemptDurationSeconds": 86400
  }
}
